{
    "name": "crptecnologia/imaginary-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Dejair Sisconeto",
            "email": "dejair.sisconeto@crptecnologia.com.br"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "~6.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpmd/phpmd": "@stable",
        "psr/log": "1.1.*",
        "friendsofphp/php-cs-fixer": "2.9.*",
        "phpunit/phpunit": "8.1.2",
        "mockery/mockery": "^1.2",
        "laravel/framework": "5.8.*"
    },
    "autoload": {
        "psr-4": {
            "CrpTecnologia\\ImaginaryClient\\": "src/"
        },
        "files": [
            "src/Laravel/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CrpTecnologia\\ImaginaryClient\\Laravel\\ImaginaryServiceProvider"
            ]
        }
    }
}
